<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #fffcf9;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.controllers.FinishOrderingController">
    <children>
        <ImageView fitHeight="600.0" fitWidth="800.0" onMouseClicked="#handleHomeButton" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../image/finish_order.png" />
            </image>
        </ImageView>
        <Label layoutX="130.0" layoutY="246.0" text="THE ORDER IS ALREADY SUCCESS" textFill="#a98a68">
            <font>
                <Font size="36.0" />
            </font>
        </Label>
        <Label layoutX="108.0" layoutY="316.0" text="YOU CAN CHECK TRACKING NUMBER IN MY ACCOUNT" textFill="#a98a68">
            <font>
                <Font size="24.0" />
            </font></Label>
      <ImageView fitHeight="50.0" fitWidth="50.0" layoutX="33.0" layoutY="20.0" onMouseClicked="#handleHomeButton" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../image/homeicon.png" />
         </image>
      </ImageView>
      <Button fx:id="exitButton" layoutX="740.0" layoutY="20.0" mnemonicParsing="false" onAction="#handleExitButton" style="-fx-background-color: ffcf9;" text="X" textFill="#a98a68">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
