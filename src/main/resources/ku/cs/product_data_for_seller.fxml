<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: ffcf9;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.controllers.ProductDataController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="800.0" opacity="0.4" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../image/finish_order.png" />
         </image>
      </ImageView>
      <HBox layoutX="407.0" layoutY="173.0" prefHeight="40.0" prefWidth="280.0" style="-fx-background-color: #F6E6CB; -fx-background-radius: 100;">
         <children>
            <Label fx:id="nameProductDataLabel" alignment="CENTER" prefHeight="40.0" prefWidth="280.0" text="nameProductDataLabel" textFill="#726659">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children></HBox>
      <HBox layoutX="407.0" layoutY="263.0" prefHeight="40.0" prefWidth="280.0" style="-fx-background-color: #F6E6CB; -fx-background-radius: 100;">
         <children>
            <Label fx:id="descriptionProductDataLabel" alignment="CENTER" prefHeight="40.0" prefWidth="280.0" text="descriptionProductDataLabel" textFill="#726659">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children></HBox>
      <HBox layoutX="407.0" layoutY="443.0" prefHeight="40.0" prefWidth="280.0" style="-fx-background-color: #F6E6CB; -fx-background-radius: 100;">
         <children>
            <Label fx:id="quantityProductDataLabel" alignment="CENTER" prefHeight="40.0" prefWidth="280.0" text="quantityProductDataLabel" textFill="#726659">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children></HBox>
      <HBox layoutX="407.0" layoutY="353.0" prefHeight="40.0" prefWidth="280.0" style="-fx-background-color: #F6E6CB; -fx-background-radius: 100;">
         <children>
            <Label fx:id="priceProductDataLabel" alignment="CENTER" prefHeight="40.0" prefWidth="280.0" text="priceProductDataLabel" textFill="#726659">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children></HBox>
      <Label layoutX="395.0" layoutY="133.0" text="PRODUCT NAME" textFill="#a98a68">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="395.0" layoutY="223.0" text="DESCRIPTION" textFill="#a98a68">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="395.0" layoutY="313.0" text="PRICE" textFill="#a98a68">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="395.0" layoutY="403.0" text="STOCK" textFill="#a98a68">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button layoutX="34.0" layoutY="527.0" mnemonicParsing="false" onAction="#handleBackToProductListButton" prefHeight="45.0" prefWidth="100.0" style="-fx-background-color: #CDBBA7; -fx-background-radius: 100;" text="BACK" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="664.0" layoutY="527.0" mnemonicParsing="false" onAction="#handleUseEditButton" prefHeight="45.0" prefWidth="100.0" style="-fx-background-color: #CDBBA7; -fx-background-radius: 100;" text="EDIT" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <ImageView fx:id="productImageView" fitHeight="300.0" fitWidth="300.0" layoutX="55.0" layoutY="150.0" pickOnBounds="true" preserveRatio="true" />
